---
layout: none
---
{
  "data": [
    {% for post in site.posts %}
    {
      "url":"{{ post.url }}",
      "title": "{{ post.title }}",
      "updated": "{{ post.date | date: "%B  %-d, %Y" }}",
      "body": "{{ post.content }}"
    }{% unless forloop.last %},{% endunless %}
    {% endfor %}
  ]
}
